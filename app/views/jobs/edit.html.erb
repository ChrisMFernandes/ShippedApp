<div class="container">
	<div class="text-center">
		<h1>Edit Job</h1>
	</div>

		<%= bootstrap_form_for(@jobs, layout: :horizontal) do |t| %>
			<p><%= t.text_field :description %></p>
			<p><%= t.text_field :origin %></p>
			<p><%= t.text_field :destination %></p>
			<p><%= t.text_field :cost %></p>
			<p><%= t.text_field :containers %></p>
		<div class="text-center">
				<%= submit_tag "Confirm" %>
		</div>
		<% end %>
</div>