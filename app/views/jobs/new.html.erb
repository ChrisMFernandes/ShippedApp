<div class="container">
	<div class="text-center">
		<h1>Add New Job</h1>
	</div>
		<div class="text-center">

			<%= bootstrap_form_for(@jobs, layout: :vertical) do |f|%>
				<label><%= f.text_field :description %></label>
				<label><%= f.text_field :origin %></label>
				<label><%= f.text_field :destination %></label>
				<label><%= f.text_field :cost %></label>
				<label><%= f.text_field :containers %></label>
		<div class="text-center">
			<%= submit_tag "Add" %>
		</div>
		<% end %>
	</div>
</div>